./rundir nts "" -assert -t:60
./rundir horn-quantifiers "" -assert -t:120
./rundir horn-prolog "" -assert -t:60
./rundir horn-smt-lib "" -assert -t:60
./rundir horn-abstract "" -assert -t:60 -abstractTO:4
