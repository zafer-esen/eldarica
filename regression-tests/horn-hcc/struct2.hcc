struct S3{
    int a;
};
struct S2{
    int a;
    struct S3 b;
};
struct S1{
    int a;
    struct S2 b;
};

thread A {
    struct S1 s;
    s.b.b.a = 41;
    s.b.a = s.b.b.a++;
    s.a = ++s.b.a;
    s.b.b.a--;
    --s.b.b.a;

    assert(s.b.b.a == 40);
    assert(s.b.a == 42);
    assert(s.a == 42);
}
